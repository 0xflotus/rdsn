config:{0,-,[]}
inject:on_rpc_call:rpc_name=rpc_config_proposal,from=m,to=r1

# an update_pc failure will lead to the failure of config-sync
inject:on_rpc_call:rpc_name=rpc_cm_update_partition_configuration,from=r1,to=m

# failure of config-sync
state:{{r1,err,0,0}}

# and so the config version will upgrade to 2
config:{2,-,[]}

# the meta will assigin primary , and state will be normal again
state:{{r1,pri,5,0},{r2,sec,5,0},{r3,sec,5,0}}
