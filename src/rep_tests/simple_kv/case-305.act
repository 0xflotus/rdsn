# wait for server ready
config:{3,r1,[r2,r3]}
state:{{r1,pri,3,0},{r2,sec,3,0},{r3,sec,3,0}}

# downgrade the primary to inactive
client:replica_config:replica=1.0.0,command=downgrade_to_inactive

# disable lb so we can test the config commands below
set:disable_load_balance=1
state:{{r1,ina,4,0},{r2,sec,3,0},{r3,sec,3,0}}

# all these command will be ignored
client:replica_config:replica=1.0.0,command=downgrade_to_secondary
client:replica_config:replica=1.0.0,command=downgrade_to_inactive
client:replica_config:replica=1.0.0,command=remove

set:disable_load_balance=0

# then a new primary will be assigned
config:{5,r3,[r2]}

# and the state will be normal again
config:{6,r3,[r1,r2]}
